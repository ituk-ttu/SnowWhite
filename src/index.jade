doctype HTML
head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE-edge")
    meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
    title SnowWhite
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
    script(src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.0/angular.min.js")
    script(src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-utils/0.1.1/angular-ui-utils.min.js")
    script(src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.11/angular-ui-router.min.js")
    script(src="assets/js/app.js")
    link(href='https://fonts.googleapis.com/css?family=Raleway:100,200,300,400,500,600,700&subset=latin,latin-ext' rel='stylesheet' type='text/css')
    link(href='assets/css/master.css' rel='stylesheet' type='text/css')
html(ng-app="SnowWhite")
